{
  "name": "babeled",
  "version": "2.0.0",
  "description": "Template for doing simple javascript things but with newer javascript syntax",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bressain/babled.git"
  },
  "scripts": {
    "build": "rm -rf ./build && babel src --out-dir build",
    "start": "nodemon --exec babel-node src",
    "test": "mocha 'src/**/*.spec.js'",
    "test:watch": "mocha 'src/**/*.spec.js' --watch",
    "lint": "eslint src/"
  },
  "author": "Bressain Dinkelman",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/node": "7.2.2",
    "@babel/polyfill": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/register": "7.4.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "eslint": "5.15.3",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "6.0.2",
    "nodemon": "1.18.10",
    "sinon": "7.3.1",
    "sinon-chai": "3.3.0"
  },
  "dependencies": {}
}
